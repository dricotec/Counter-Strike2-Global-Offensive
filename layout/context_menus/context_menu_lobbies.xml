<root>
    <styles>
        <include src="file://{resources}/styles/gamestyles.css" />
        <include src="file://{resources}/styles/context_menus/context_menu_lobbies.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/context_menus/context_menu_lobbies.js" />
    </scripts>
    <Panel onload="ContextMenuLobbies.Init()" useglobalcontext="true" acceptsinput="true" acceptsfocus="true">
        <Label class="context-menu-lobbies__title stratum-medium" text="#contextmenu_lobby-count" />
        <Panel id="id-context-menu-lobbies" class="context-menu-lobbies" />
        <Button id="id-context-menu-lobbies-new-btn" class="context-menu-lobbies__new-lobbies-btn hide" onactivate="ContextMenuLobbies.OnPressGotoNew()">
            <Image texturewidth="10" textureheight="10" src="file://{images}/icons/ui/back.svg" />
            <Label text="New Lobbies" />
        </Button>
    </Panel>
</root>